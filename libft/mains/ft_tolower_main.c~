#include <stdio.h>

int	ft_tolower(int c)
{
	if (c >= 'A' && c <= 'Z')
	{
		c = c + 32;
		return (c);
	}
	else
		return (c);
}

int	main()
{
	int	c;

	c = 'Z';
	printf("%d\n", ft_tolower(c));
	return (0);
}
