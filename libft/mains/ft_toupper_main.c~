#include <stdio.h>

int	ft_toupper(int c)
{
	if (c > 'a' && c < 'z')
	{
		c = c - 32;
		return (c);
	}
	else
		return (c);
}

int	main()
{
	int	c;

	c = 'c';
	printf("ft_toupper: %d\n", ft_toupper(c));
	return (0);
}
